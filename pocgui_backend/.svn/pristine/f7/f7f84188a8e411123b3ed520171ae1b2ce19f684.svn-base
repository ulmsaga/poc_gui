package com.mobigen.cdev.datagw.module.collect.interfacesource.dto;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;
import com.mobigen.cdev.datagw.core.base.dto.BaseObject;
import com.mobigen.cdev.datagw.core.exception.RsRuntimeException;
import com.mobigen.cdev.datagw.core.file.dto.FileDto;
import com.mobigen.cdev.datagw.core.security.util.aria.ARIAEngine;

@JsonInclude(Include.ALWAYS)
public class InterfaceAddedFileDto extends BaseObject {
  private static final long serialVersionUID = 1867501618832073498L;

  private String added_file_seq;
  private String interface_type;
  private String interface_id;
  private String client_file_name;
  private String file_path;
  private String file_name;
  private String file_size;
  private String file_desc;

  public String getAdded_file_seq() {
    return added_file_seq;
  }
  public void setAdded_file_seq(String added_file_seq) {
    this.added_file_seq = added_file_seq;
  }
  public String getInterface_type() {
    return interface_type;
  }
  public void setInterface_type(String interface_type) {
    this.interface_type = interface_type;
  }
  public String getInterface_id() {
    return interface_id;
  }
  public void setInterface_id(String interface_id) {
    this.interface_id = interface_id;
  }
  public String getClient_file_name() {
    return client_file_name;
  }
  public void setClient_file_name(String client_file_name) {
    this.client_file_name = client_file_name;
  }
  public String getFile_path() {
    return file_path;
  }
  public void setFile_path(String file_path) {
    this.file_path = file_path;
  }
  public String getFile_name() {
    return file_name;
  }
  public void setFile_name(String file_name) {
    try {
      this.file_name = ARIAEngine.getEncrypt(file_name, FileDto.FILE_ENC_KEY);
    } catch (Exception e) {
      throw new RsRuntimeException(e);
    }
  }
  public String getFile_size() {
    return file_size;
  }
  public void setFile_size(String file_size) {
    this.file_size = file_size;
  }
  public String getFile_desc() {
    return file_desc;
  }
  public void setFile_desc(String file_desc) {
    this.file_desc = file_desc;
  }
  
}
