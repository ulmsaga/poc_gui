package com.mobigen.cdev.datagw.module.test.repository;

import java.util.List;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.stereotype.Repository;

import com.mobigen.cdev.datagw.module.common.dto.user.UserInfoDto;

@Repository
public class UserManageRepositoryImpl implements UserManageRepository {

	private final SqlSessionTemplate sqlSessionTemplateGuiweb;
	private final String namespace = "com.mobigen.cdev.mapper.mysql.module.common";
	
	public UserManageRepositoryImpl(SqlSessionTemplate sqlSessionTemplateGuiweb) {
		this.sqlSessionTemplateGuiweb = sqlSessionTemplateGuiweb;
	}
	
	@Override
	public int mergeUserInfoFromTpani(List<UserInfoDto> userInfoDtoList) {
		return sqlSessionTemplateGuiweb.insert(namespace + ".insertUserInfo", userInfoDtoList);
	}

}
