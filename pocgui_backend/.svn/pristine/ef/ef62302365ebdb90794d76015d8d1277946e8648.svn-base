package com.mobigen.cdev.datagw.module.datamanages.interfacesource.dto;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;
import com.mobigen.cdev.datagw.core.base.dto.BaseObject;

@JsonInclude(Include.ALWAYS)
public class JsonPropertyDefDto extends BaseObject {
	private static final long serialVersionUID = -142572454436830797L;

  private String field;
  private String header;
  private String json_key;
  private String key_desc;
  private String key_name;
  private String validator;
  private String json_columns;
  private String data_type;
  private String display_attribute;
  // properties style
  private String minWith;
  private String textAlign;
  private String width;
  private String suppressSizeToFit;
  // 기존 등록 정보 COPY시 초기화 항목
  private String defaultEmpty;

  public String getField() {
    return field;
  }
  public void setField(String field) {
    this.field = field;
  }
  public String getHeader() {
    return header;
  }
  public void setHeader(String header) {
    this.header = header;
  }
  
  public String getKey_desc() {
    return key_desc;
  }
  public void setKey_desc(String key_desc) {
    this.key_desc = key_desc;
  }
  public String getKey_name() {
    return key_name;
  }
  public void setKey_name(String key_name) {
    this.key_name = key_name;
  }
  public String getValidator() {
    return validator;
  }
  public void setValidator(String validator) {
    this.validator = validator;
  }
  public String getJson_columns() {
    return json_columns;
  }
  public void setJson_columns(String json_columns) {
    this.json_columns = json_columns;
  }
  public String getData_type() {
    return data_type;
  }
  public void setData_type(String data_type) {
    this.data_type = data_type;
  }
  public String getDisplay_attribute() {
    return display_attribute;
  }
  public void setDisplay_attribute(String display_attribute) {
    this.display_attribute = display_attribute;
  }
  public String getMinWith() {
    return minWith;
  }
  public void setMinWith(String minWith) {
    this.minWith = minWith;
  }
  public String getTextAlign() {
    return textAlign;
  }
  public void setTextAlign(String textAlign) {
    this.textAlign = textAlign;
  }
  public String getWidth() {
    return width;
  }
  public void setWidth(String width) {
    this.width = width;
  }
  public String getSuppressSizeToFit() {
    return suppressSizeToFit;
  }
  public void setSuppressSizeToFit(String suppressSizeToFit) {
    this.suppressSizeToFit = suppressSizeToFit;
  }
  public String getJson_key() {
    return json_key;
  }
  public void setJson_key(String json_key) {
    this.json_key = json_key;
  }
  public String getDefaultEmpty() {
    return defaultEmpty;
  }
  public void setDefaultEmpty(String defaultEmpty) {
    this.defaultEmpty = defaultEmpty;
  }
}
